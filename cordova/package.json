{
  "name": "wander2k.vue.cordova.demo",
  "displayName": "VueCordovaExample",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "build:release": "cordova build --release --device --buildConfig=./build.json android",
    "build:debug": "cordova build --debug --device --buildConfig=./build.json android",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "cordova-android": "^9.1.0",
    "cordova-browser": "~6.0.0",
    "cordova-plugin-whitelist": "~1.3.5"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android",
      "browser"
    ]
  }
}